{
  "manifest_version": 3,
  "name": "flip-time",
  "description": "minimalist flip clock displays time on each new tab",
  "version": "1.4.0",
  "icons": {
    "128": "img/icon128.png"
  },
  "action": {
    "default_icon": "img/icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"]
}
